# schema file of the microservice

# defines data types, queries and mutations
# this can be done in a separate files as long as they are in this folder and
# end with .graphqls

enum MediaType {
  AUDIO,
  VIDEO,
  IMAGE,
  PRESENTATION,
  DOCUMENT,
  URL
}

type MediaRecord {
  mediaId: ID!
  mediaName: String!
  mediaType: MediaType!
}

type Query {
  media: [MediaRecord!]!
}